@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <h4>Version @ViewData["changeNumber"] of Cloud @ViewData["cloud"]</h4>
</div>
<br /><br />
<div class="container">
    <div class="row">
        <div class="col-sm-4">
            <div class="input-group mb-3">
                <select id='selST' multiple="multiple">
                    @if (ViewBag.ServiceTags != null)
                    {
                        foreach (ValuesModel st in ViewBag.ServiceTags)
                        {
                            <option value="@st.id">@st.name</option>
                        }
                    }
                    else
                    {
                        <option disabled selected>No data available</option>
                    }
                </select>
            </div>
            <div id='links'>
                <div id='downloadlink' data-url='@ViewData["env"]'></div>
                <div id='armdownloadlink' data-url='@ViewData["env"]'></div>
                <div id='deploytoazure' data-url='@ViewData["env"]'></div>
                <div id='opensenselink' data-url='@ViewData["env"]'></div>
            </div>
        </div>
        <div class="col-sm-8" id="result">
            <div class="mt-5 ml-5 pt-5 pl-5">
                <svg xmlns="http://www.w3.org/2000/svg" transform="scale(2.5)" width="100" height="77.43" viewBox="0 0 19.68 15.24"><path d="M9.105 14.43l4.642-.82.043-.01-2.387-2.84a403.945 403.945 0 0 1-2.387-2.853c0-.014 2.465-6.802 2.479-6.826.004-.008 1.682 2.888 4.066 7.02l4.09 7.09.031.054-7.587-.001-7.587-.001 4.597-.812zM0 13.566c0-.004 1.125-1.957 2.5-4.34L5 4.893l2.913-2.445C9.515 1.104 10.83.002 10.836 0a.512.512 0 0 1-.047.118L7.625 6.903l-3.107 6.663-2.259.003c-1.242.002-2.259 0-2.259-.004z" fill="#0089d6"/></svg>
            </div>
        </div>
    </div>
</div>

